node {
    stage("Checkout") {
        checkout scm
    }
    stage("Building image") {
        dir("employee") {
            dockerImage = docker.build('rachitsrivastava007/${JOB_NAME}', '-f Dockerfile .')
        }
    }
    stage('Deploy Image') {
      steps{
        script {
          docker.withRegistry( '', rachitsrivastava007 ) {
            dockerImage.push()
          }
        }
      }
    }
}
